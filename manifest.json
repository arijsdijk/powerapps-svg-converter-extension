{
  "manifest_version": 3,
  "name": "PowerApps SVG Converter",
  "version": "1.0.0",
  "description": "A browser extension to convert SVG files for use in PowerApps",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}